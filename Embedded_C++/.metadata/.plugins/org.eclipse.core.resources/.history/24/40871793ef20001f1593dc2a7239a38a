#include "main.h"
#define PIN_NUMBER 2

int main(void)
{
	RCC->APB2ENR =(1 << 2);
//	GPIOC->CRH = 0x00300000; // pin 13
	GPIOA->CRL = (3 << 4 );// 0x00000030

  while (1)
  {
//	  GPIOC->ODR = (1 << 13);
	  GPIOA->ODR = (1 << PIN_NUMBER);
	  for(int i = 0; i < 100000; i++);
//	  GPIOC->ODR &=~(1 << 13);
	  GPIOA->ODR &=~ (1 << PIN_NUMBER);
	  for(int i = 0; i < 100000; i++);
  }

}

