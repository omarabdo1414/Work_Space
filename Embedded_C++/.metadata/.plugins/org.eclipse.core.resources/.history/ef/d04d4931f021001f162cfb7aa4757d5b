
#include "main.h"
#include "dio.h"
#include "Std_Types.h"
#include "bit_math.h"

#define PUSH_BUTTON_CHANNEL ChannelPA2
#define LED_CHANNEL 		ChannelPB4

int main(void)
{
  RCC->APB2ENR = (1 << 2) | (1 << 3);

  GPIOA->CRL |= (0x3 << (4 * 2));
  GPIOB->CRL |= (0x4 << (4 * 4));

  while (1)
  {
	  Dio_LevelType button_state = Dio_ReadChannel(PUSH_BUTTON_CHANNEL);
  }

}
